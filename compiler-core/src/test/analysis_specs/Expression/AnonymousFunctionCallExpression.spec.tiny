multiplyByTwo = [n: Int] -> n * 2
main = (multiplyByTwo.[n = 3]) * 2

////
// error = InvalidAnonymousFunctionCallException

multiplyByTwo = [n: Int] -> n * 2
main = multiplyByTwo.![n = 3]

////
// error = InvalidAnonymousFunctionCallException

multiplyByTwo = [n: Int] -> n * 2
main = multiplyByTwo.[]

////
// error = InvalidAnonymousFunctionCallException

multiplyByTwo = [n: Int] -> n * 2
main = multiplyByTwo.[n = ()]

////
// error = InvalidAnonymousFunctionCallException

multiplyByTwo = [n: Int] -> n * 2
main = multiplyByTwo.

////

main! = (! -> new intBox).!

////
// error = InvalidAnonymousFunctionCallException

main! = (! -> new intBox).

////
