// error = OperatorSignatureNotFoundException

returnOne = -> 1
main = returnOne * 2

////
// error = InvalidAnonymousFunctionCallException

main = 3.

////
// error = InvalidAnonymousFunctionCallException

returnOne = -> 1
main = returnOne.!

////
// error = InvalidAnonymousFunctionCallException

returnOne = -> 1
main = returnOne.[]

